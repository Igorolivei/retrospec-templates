require 'spec_helper'

describe '<%= name %>' do
  <%- if type == :rvalue -%>
  it { is_expected.not_to eq(nil) }
  <%- else               -%>
  it { is_expected.to eq(nil) }
  <%- end                -%>
  it { is_expected.to run.with_params().and_raise_error(Puppet::ParseError, /wrong number of arguments/i) }
end
